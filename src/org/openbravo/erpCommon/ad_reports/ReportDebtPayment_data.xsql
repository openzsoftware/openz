  <SqlMethod name="selectReportWithReportingDate" type="preparedStatement" return="multiple">
    <SqlMethodComment></SqlMethodComment>
    <Sql>
    <![CDATA[
SELECT '' AS BANKACC,
      '' as ACCOUNTSTR,
      (CASE WHEN I.C_INVOICE_ID IS NULL THEN '0' ELSE I.C_INVOICE_ID END) AS C_INVOICE_ID,
       (CASE WHEN DP.C_DEBT_PAYMENT_ID IS NULL THEN '0' ELSE DP.C_DEBT_PAYMENT_ID END) AS C_DEBT_PAYMENT_ID,
        B.Value||'-'||B.NAME AS BPARTNER,
         (CASE WHEN I.DOCUMENTNO IS NOT NULL THEN I.DOCUMENTNO END) AS INVOICE,
          coalesce(i.schedtransactiondate,dp.dateplanned) as DATEPLANNED,
      (case when ad_get_docbasetype(i.c_doctype_id) in ('APC','ARC') then -1 else 1 end) * i.grandtotal as amount,
       (case when ad_get_docbasetype(i.c_doctype_id) in ('APC','ARC') then -1 else 1 end) * i.WRITEOFFAMT as WRITEOFFAMT,
        C.ISO_CODE AS CURRENCY,
         DP.Updated, DP.Updatedby,
       i.totalpaid AS DEBTCANCEL,
      (case when  i.ispaid='N' then ((case when ad_get_docbasetype(i.c_doctype_id) in ('APC','ARC') then -1 else 1 end) * (i.grandtotal-i.WRITEOFFAMT-i.discountamt)) - i.totalpaid else 0 end) AS DEBTGENERATE,
       COALESCE(RLT.NAME, RL.NAME) AS PAYMENTRULE,
      (CASE C_DEBT_PAYMENT_STATUS(DP.C_Settlement_Cancel_ID, DP.CANCEL_PROCESSED, DP.GENERATE_PROCESSED, i.ISPAID, DP.ISVALID, 
      DP.C_CASHLINE_ID, DP.C_BANKSTATEMENTLINE_ID) WHEN 'I' THEN AD_MESSAGE_GET2('DPR_INVALID',?) WHEN 'C' THEN AD_MESSAGE_GET2('DPR_CONCILIATED',?) WHEN 'W' THEN AD_MESSAGE_GET2('DPR_CANCELLED',?) WHEN 'A' THEN AD_MESSAGE_GET2('DPR_SETTLE',?) WHEN 
      'P' THEN  case when i.c_dunning_id is not null then C_DUNNING_STATUS(i.c_dunning_id , ?) else  AD_MESSAGE_GET2('DPR_PENDING',?) END END) AS STATUS, 
      DP.isapproved, (CASE DP.isapproved WHEN 'Y' THEN (TO_CHAR(DP.APPROVALDATE,'DD.MM.YYYY')||' '||(SELECT name from ad_user where ad_user_id=DP.APPROVEDBY)) ELSE (' ') END) AS appby,
      I.issotrx as approvaldisabled,
      DP.DESCRIPTION, AD_MESSAGE_GET2(i.ISPAID, ?) AS ISPAID, 
      (CASE DP.ISRECEIPT WHEN 'Y' THEN 'SalesInvoice' ELSE 'PurchaseInvoice' END) AS INVOICE_TYPE, 
      COALESCE(RLT2.NAME, RL2.NAME) AS DPSTATUS, URS.NAME AS SALESREPNAME,
      zssi_getprojectorcostcentername(i.c_project_id,i.a_asset_id) as PROJECT,
      zssi_getshortorg(I.ad_org_id) as ORG,
      I.Dateinvoiced as IDATE
      FROM cDebtPaymentReportingDateFilter(?) DP left join C_BPARTNER B    on DP.C_BPARTNER_ID = B.C_BPARTNER_ID
                             left join C_BANKACCOUNT BA on DP.C_BANKACCOUNT_ID = BA.C_BANKACCOUNT_ID
                             left join C_BANK BANK ON BA.C_BANK_ID = BANK.C_BANK_ID
                             left join C_INVOICE I     on DP.C_INVOICE_ID = I.C_INVOICE_ID left join AD_USER URS on I.SALESREP_ID = URS.AD_USER_ID,
           AD_REF_LIST RL    left join AD_REF_LIST_TRL RLT on RL.AD_REF_LIST_ID = RLT.AD_REF_LIST_ID
                                                              AND RLT.AD_LANGUAGE = ?,
           AD_REF_LIST RL2    left join AD_REF_LIST_TRL RLT2 on RL2.AD_REF_LIST_ID = RLT2.AD_REF_LIST_ID
                                                              AND RLT2.AD_LANGUAGE = ?,
           C_CURRENCY C
      WHERE DP.C_CURRENCY_ID = C.C_CURRENCY_ID 
      AND DP.PAYMENTRULE = RL.VALUE
      AND RL.AD_REFERENCE_ID = '195'
      AND DP.STATUS = RL2.VALUE
      AND RL2.AD_REFERENCE_ID = '800070'
      AND DP.AD_Client_ID IN ('1') 
      AND DP.AD_Org_ID IN ('1') 
      AND DP.IsActive='Y'
      AND DP.IsValid='Y'
      GROUP BY bank.name,i.ispaid,bank.swiftcode,bank.codebank,ba.iban,ba.accountno,i.grandtotal,i.writeoffamt,i.discountamt,i.totalpaid,i.documentno, dp.c_debt_payment_id,i.dateinvoiced,i.ad_org_id,I.C_PROJECT_ID,i.a_asset_id,DP.UPDATED, DP.UPDATEDBY, DP.APPROVEDBY,dp.APPROVALDATE,(BANK.NAME || '/' || BANK.CODEBRANCH || BANK.DIGITCONTROL || BA.CODEACCOUNT || '.' || BA.DIGITCONTROL),(CASE BA.showspanish WHEN 'Y' then BA.CODEACCOUNT ELSE CASE BA.SHOWGENERIC when 'Y' then GENERICACCOUNT ELSE CASE BA.SHOWIBAN WHEN 'Y' THEN BA.IBAN END END END),
      CASE WHEN I.C_INVOICE_ID IS NULL THEN '0' ELSE I.C_INVOICE_ID END, (CASE WHEN DP.C_DEBT_PAYMENT_ID IS NULL THEN '0' ELSE DP.C_DEBT_PAYMENT_ID END), B.Value,B.NAME, (CASE WHEN I.DOCUMENTNO IS NOT NULL THEN I.DOCUMENTNO||' ('||I.DATEINVOICED||')' END), coalesce(i.schedtransactiondate,dp.dateplanned), DP.AMOUNT,
      DP.WRITEOFFAMT, C.ISO_CODE, RLT.NAME, RL.NAME, RLT2.NAME, RL2.NAME, DP.C_Settlement_Cancel_ID,
      DP.CANCEL_PROCESSED, DP.GENERATE_PROCESSED, DP.isapproved,I.issotrx,i.c_doctype_id,i.c_dunning_id,DP.ISPAID, DP.ISVALID, DP.C_CASHLINE_ID, DP.C_BANKSTATEMENTLINE_ID, DP.DESCRIPTION, (CASE DP.ISRECEIPT WHEN 'Y' THEN 'SalesInvoice' ELSE 'PurchaseInvoice' END), DP.STATUS, URS.NAME

      ORDER BY 1,STATUS, coalesce(i.schedtransactiondate,dp.dateplanned),i.documentno
     ]]></Sql>
