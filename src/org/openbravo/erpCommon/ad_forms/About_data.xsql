<SqlClass name="AboutData" package="org.openbravo.erpCommon.ad_forms">
  <SqlClassComment></SqlClassComment>
  <SqlMethod name="select" type="preparedStatement" return="single">
    <SqlMethodComment></SqlMethodComment>
    <Sql>
        SELECT VERSION, VERSION_LABEL, VERSION_ID, 
               '' as LANG, '' as TRANSLATEDBY
          FROM AD_MODULE
         WHERE AD_MODULE_ID ='0'
     </Sql>
   </SqlMethod>
   <SqlMethod name="selectTranslators" type="preparedStatement" return="multiple">
    <SqlMethodComment></SqlMethodComment>
    <Sql>
        SELECT NAME AS LANG, TRANSLATEDBY
          FROM AD_LANGUAGE
         WHERE TRANSLATEDBY IS NOT NULL
           AND (ISSYSTEMLANGUAGE  = 'Y'
                OR ISBASELANGUAGE = 'Y')
         ORDER BY NAME
     </Sql>
   </SqlMethod>
   
  <SqlMethod name="set" type="constant" return="multiple">
      <SqlMethodComment></SqlMethodComment>
      <Sql></Sql>
  </SqlMethod>
   
</SqlClass>
