<SqlClass name="DocMovementData" package="org.openbravo.erpCommon.ad_forms">
  <SqlClassComment></SqlClassComment>
  <SqlMethod name="select" type="preparedStatement" return="multiple">
    <SqlMethodComment></SqlMethodComment>
    <Sql>
      <![CDATA[
        SELECT M.AD_CLIENT_ID, M.AD_ORG_ID, M.AD_ORGTRX_ID, M.C_PROJECT_ID, M.C_CAMPAIGN_ID,
        M.C_ACTIVITY_ID, M.USER1_ID, M.USER2_ID, M.NAME, M.POSTED, M.MOVEMENTDATE
        FROM M_MOVEMENT M
        WHERE AD_CLIENT_ID=? 
        AND M_MOVEMENT_ID=?
     ]]>
     </Sql>
     <Parameter name="client"/>
     <Parameter name="id"/>
   </SqlMethod>
   </SqlClass>
