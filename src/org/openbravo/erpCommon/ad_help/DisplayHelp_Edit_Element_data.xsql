        FROM AD_TAB left join AD_TAB_TRL on AD_TAB.AD_TAB_ID = AD_TAB_TRL.AD_TAB_ID 
                                        AND AD_TAB_TRL.AD_LANGUAGE =?
        WHERE AD_TAB.AD_TAB_ID = ?
        AND AD_TAB.ISACTIVE = 'Y'
        ]]>
      </Sql>
        <Parameter name="adLanguage"/>
        <Parameter name="id"/>        
   </SqlMethod>
