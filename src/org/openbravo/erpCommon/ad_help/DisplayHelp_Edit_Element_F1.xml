<REPORT>
  <template file="DisplayHelp_Edit_Element_F1.html"/>
  <PARAMETER id="paramLanguage" name="language" default=""/>
  <PARAMETER id="paramDirectory" name="directory" default=""/>
	<PARAMETER id="paramCSS" name="theme" attribute="href" replace="Default"/>
  <PARAMETER id="fieldSave" name="save" attribute="value" default=""/>
  <PARAMETER id="fieldEdit" name="edit" attribute="value" default=""/>
  <PARAMETER id="paramField" name="field" attribute="value" default=""/>
  <PARAMETER id="messageBoxID" name="messageType" attribute="class" replace="yy" default="HIDDEN"/>
  <PARAMETER id="messageBoxIDTitle" name="messageTitle" default=""/>
  <PARAMETER id="messageBoxIDMessage" name="messageMessage" default=""/>
  <structure name="structure1">
    <FIELD id="fieldId" attribute="value">id</FIELD>
    <FIELD id="fieldOriginalName">originalname</FIELD>
    <FIELD id="fieldName" attribute="value">name</FIELD>
    <FIELD id="idfieldHelp">help</FIELD>
    <SECTION id="sectionDetail"/>
  </structure>
  <structure name="structure2">
    <FIELD id="fieldFieldId" attribute="onclick" replace="xx">id</FIELD>
    <FIELD id="fieldFieldId" attribute="onclick" replace="yy">id</FIELD>
    <FIELD id="fieldOriginalName1">originalname</FIELD>
    <FIELD id="fieldName1" attribute="value">name</FIELD>
    <FIELD id="fieldHelp1">help</FIELD>
    <SECTION id="sectionField"/>
  </structure>
  <DISCARD id="discard"/>
</REPORT>
