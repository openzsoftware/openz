       C_LOCATION.POSTAL, C_LOCATION.CITY,zssi_getOwnCodeAtPartnerSide(M_INOUT.ad_org_id,C_BPARTNER.c_bpartner_id) as owncodeatpartnersite,
       zssi_getusernamecomplete(m_inout.salesrep_id,$P{LANGUAGE}) as ourrepresentative, zssi_getuseremail(m_inout.salesrep_id) as ourrepemail, zssi_getuserphone(m_inout.salesrep_id) as ourrepphone,
        C_BPARTNER_LOCATION.PHONE,C_BPARTNER_LOCATION.FAX,C_BPARTNER.URL, C_BPARTNER_LOCATION.uidnumber, null as  cosunit, null as costcenter,
        C_Location_Description(AD_ORGINFO.C_LOCATION_ID) AS LOCATION, null AS CURRENCY_CODE,null AS SYMBOL,
        M_INOUT.DESCRIPTION, zssi_strDate(M_INOUT.MOVEMENTDATE, $P{LANGUAGE}) as DATEORDERED, M_INOUT.DOCUMENTNO, zssi_strDate(m_inout.shipdate,$P{LANGUAGE}) as datepromised, zssi_2html('') AS PAYTERM,
        zssi_strDate(current_date,$P{LANGUAGE}) as curdate,
        zssi_getlocationcity(M_INOUT.DELIVERY_LOCATION_ID) as delcity,
        zssi_getlocationcountry(M_INOUT.DELIVERY_LOCATION_ID,$P{LANGUAGE}) as delcountry,
        zssi_getlocationline1(M_INOUT.DELIVERY_LOCATION_ID) as deladdr1,
        zssi_getlocationline2(M_INOUT.DELIVERY_LOCATION_ID) as deladdr2,
        zssi_getlocationpostal(M_INOUT.DELIVERY_LOCATION_ID) as delpostal,
        null as paymentterm,
        zssi_getlocationcountry(M_INOUT.C_BPARTNER_LOCATION_ID,$P{LANGUAGE}) as bpcountry,
        zssi_getexportterm(m_inout.C_BPARTNER_LOCATION_ID, m_inout.c_doctype_id,$P{LANGUAGE}) as exportterm,
        zssi_getlocationeori(M_INOUT.C_BPARTNER_LOCATION_ID) as eori,
        zssi_getincoterms(M_INOUT.c_incoterms_id) as incoterms,
        zssi_getshipper(M_INOUT.m_shipper_id) as shipper,
        zssi_getdeliverymethod(M_INOUT.deliveryviarule,$P{LANGUAGE}) as deliverymethod,
