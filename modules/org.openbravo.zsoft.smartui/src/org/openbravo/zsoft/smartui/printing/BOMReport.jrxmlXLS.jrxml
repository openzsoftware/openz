	<queryString>
		<![CDATA[SELECT
        zssi_getproductname($P{M_PRODUCT_ID},$P{AD_LANGUAGE}) as assembly,
        zssi_getelementtextbycolumname('bom_title',$P{AD_LANGUAGE}) as title,
        zssi_getproductkey($P{M_PRODUCT_ID}) as productkey
