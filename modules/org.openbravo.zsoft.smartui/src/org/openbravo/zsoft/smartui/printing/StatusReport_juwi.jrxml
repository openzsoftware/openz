	</parameter>
	<queryString>
		<![CDATA[SELECT

ord.c_order_id as ID,
--Page Header Left
ord.name as NAME,
prj.value AS PROJECTNR,
(select ad_user.name from ad_user where ord.salesrep_id=ad_user.ad_user_id) as PROJECTMGR,
bp.name as CLIENT,
to_char(ost.updated,'DD.MM.YYYY') AS UPDATEDON,
ord.documentno as DOCNO,
zssi_strNumber(ord.totallines,$P{LANGUAGE})  as SUMNET,
C_CURRENCY.cursymbol as CURSYMBOL,
jt.name as TEAM,
au.name AS CLIENT_CONTACT,
--Page Header Right
 zssi_getoverviewdesign(ost.juwimm_time, 'BBFB55FDBAA64C0EB46F026460AE8285') as TIME,
 zssi_getoverviewdesign(ost.juwimm_resources, '7C374D10EA044BE3B252D6654A7AA5E1') as RESSOURCES,
 zssi_getoverviewdesign(ost.juwimm_budget, '76CDE2B245D44726AE30457453FF622E') as BUDGET,
to_char(ost.juwimm_eststart,'DD.MM.YYYY') as START,
to_char(ost.juwimm_nextimpdate,'DD.MM.YYYY') AS NID,
to_char(ost.juwimm_estend,'DD.MM.YYYY') as END,
ost.juwimm_completion AS COMPLETION,
--Column Header
ost.juwimm_description as SHORTDESC,
--DETAIL AREA MILESTONES
ost.juwimm_milestone1 as MS1,
to_char(ost.juwimm_ms1planneddate,'DD.MM.YYYY') as MS1P,
to_char(ost.juwimm_ms1estdate,'DD.MM.YYYY') as MS1E,
ost.juwimm_milestone2 as MS2,
to_char(ost.juwimm_ms2planneddate,'DD.MM.YYYY') as MS2P,
to_char(ost.juwimm_ms2estdate,'DD.MM.YYYY') as MS2E,
ost.juwimm_milestone3 as MS3,
to_char(ost.juwimm_ms3planneddate,'DD.MM.YYYY') as MS3P,
to_char(ost.juwimm_ms3estdate,'DD.MM.YYYY') as MS3E,
ost.juwimm_milestone4 as MS4,
to_char(ost.juwimm_ms4planneddate,'DD.MM.YYYY') as MS4P,
to_char(ost.juwimm_ms4estdate,'DD.MM.YYYY') as MS4E,
ost.juwimm_milestone5 as MS5,
to_char(ost.juwimm_ms5planneddate,'DD.MM.YYYY') as MS5P,
to_char(ost.juwimm_ms5estdate,'DD.MM.YYYY') as MS5E,
ost.juwimm_milestone6 as MS6,
to_char(ost.juwimm_ms6planneddate,'DD.MM.YYYY') as MS6P,
to_char(ost.juwimm_ms6estdate,'DD.MM.YYYY') as MS6E,
ost.juwimm_milestone7 as MS7,
to_char(ost.juwimm_ms7planneddate,'DD.MM.YYYY') as MS7P,
to_char(ost.juwimm_ms7estdate,'DD.MM.YYYY') as MS7E,
'End' as MS8,
to_char(ost.juwimm_plannedend,'DD.MM.YYYY') as MS8P,
to_char(ost.juwimm_estend,'DD.MM.YYYY') as MS8E,
--DETAIL AREA OUTPUTREMARKS
ost.juwimm_ouputremarks as OUTPUTREMARKS,
--STATE / DAILY REPORTS
ost.juwimm_lastaction as LASTACTION,
ost.juwimm_nextaction as NEXTACTION,
ost.juwimm_issues as ISSUES,
ost.juwimm_decisions as DECISIONS,
--IMAGES PFADE
coalesce(zssi_getdelaydesign(to_number(ost.juwimm_ms1estdate - ost.juwimm_ms1planneddate)),'') as MS1R,
coalesce(zssi_getdelaydesign(to_number(ost.juwimm_ms2estdate - ost.juwimm_ms2planneddate)),'') as MS2R,
coalesce(zssi_getdelaydesign(to_number(ost.juwimm_ms3estdate - ost.juwimm_ms3planneddate)),'') as MS3R,
coalesce(zssi_getdelaydesign(to_number(ost.juwimm_ms4estdate - ost.juwimm_ms4planneddate)),'') as MS4R,
coalesce(zssi_getdelaydesign(to_number(ost.juwimm_ms5estdate - ost.juwimm_ms5planneddate)),'') as MS5R,
coalesce(zssi_getdelaydesign(to_number(ost.juwimm_ms6estdate - ost.juwimm_ms6planneddate)),'') as MS6R,
coalesce(zssi_getdelaydesign(to_number(ost.juwimm_ms7estdate - ost.juwimm_ms7planneddate)),'') as MS7R,
coalesce(zssi_getdelaydesign(to_number(ost.juwimm_estend - ost.juwimm_plannedend)),'') as MS8R,

--ADDITIONS
ord.ad_client_id,
ord.ad_org_id,
ord.updated,
ord.c_order_id AS juwimm_orderstatus_controlling_v_id,
ord.docstatus,
ord.docaction,
ord.c_project_id,
prj.name AS project_name,
ost.juwimm_plannedstart,
ost.isactive AS statusisactive,
ost.created AS statuscreated,
ost.createdby AS statuscreatedby,
ost.updatedby AS statusupdatedby,
ost.juwimm_team_id,
ost.juwimm_plannedgolive,
ost.juwimm_estgolive
FROM
	c_order ord
   	LEFT JOIN c_project prj ON prj.c_project_id = ord.c_project_id
   	LEFT JOIN juwimm_orderstatus ost ON ost.c_order_id = ord.c_order_id
	LEFT JOIN c_bpartner bp ON bp.c_bpartner_id=ord.c_bpartner_id
	LEFT JOIN juwimm_team jt on jt.juwimm_team_id=ost.juwimm_team_id
	LEFT JOIN ad_user au on au.ad_user_id=ord.ad_user_id
	LEFT JOIN C_currency on ord.c_currency_id=c_currency.c_currency_id
WHERE ord.C_ORDER_ID IN ($P{DOCUMENT_ID})]]>
	</queryString>
