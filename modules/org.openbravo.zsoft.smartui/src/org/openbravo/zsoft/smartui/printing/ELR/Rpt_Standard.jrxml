	zssi_getlocationcountry(M_INOUT.C_BPARTNER_LOCATION_ID,$P{LANGUAGE}) as bpcountry,
        zssi_getexportterm(m_inout.C_BPARTNER_LOCATION_ID, m_inout.c_doctype_id,$P{LANGUAGE}) as exportterm,
	zssi_getlocationeori(M_INOUT.C_BPARTNER_LOCATION_ID) as eori,
	zssi_getincoterms(M_INOUT.c_incoterms_id) as incoterms,
	zssi_getshipper(M_INOUT.m_shipper_id) as shipper,
	zssi_getdeliverymethod(M_INOUT.deliveryviarule,$P{LANGUAGE}) as deliverymethod,
        '' as paymethod,
	zssi_strint(m_inout.weight,$P{LANGUAGE}) as weight,
	zssi_getuom(m_inout.weight_uom,$P{LANGUAGE}) as weight_uom,
        m_inout.trackingno as trackingno,
	m_inout.shippingto as shippingto,
	ad_image.binarydata as logo,
	m_inout.issotrx as issotrx,
        zssi_GetDocText(M_INOUT.c_doctype_id,$P{LANGUAGE}) as title, m_inout.c_doctype_id as doctype_id,
	null as dbt,
        zspr.addressheader,  zspr.address1, zspr.address2, zspr.address3, zspr.address4, zspr.address5, zspr.address6, zspr.address7, zspr.address8, zspr.address9, zspr.footer1, zspr.footer2, zspr.footer3, zspr.footer4, zspr.footer5, zspr.onelineadr, zspr.isfootergroupstyle,
m_inout.poreference,
zssi_getElementTextByColumname('yourreference',$P{LANGUAGE}) as refhead,
	-- Spezielle Dokumentendaten (je nach Dokument und Einstellung vorhanden oder nicht)
		zssi_2html(zssi_getTextModuleFromDoc(M_INOUT.M_INOUT_ID,'SHIPMENT','N', $P{LANGUAGE})) as textmodule1,  -- Textmodul oben shipment
		zssi_2html(zssi_getTextModuleFromDoc(M_INOUT.M_INOUT_ID,'SHIPMENT','Y', $P{LANGUAGE})) as textmodule2  --Textmodul unten shipment
FROM M_INOUT LEFT JOIN C_BPARTNER ON M_INOUT.C_BPARTNER_ID = C_BPARTNER.C_BPARTNER_ID
                 LEFT JOIN C_BPARTNER_LOCATION ON M_INOUT.C_BPARTNER_LOCATION_ID = C_BPARTNER_LOCATION.C_BPARTNER_LOCATION_ID
