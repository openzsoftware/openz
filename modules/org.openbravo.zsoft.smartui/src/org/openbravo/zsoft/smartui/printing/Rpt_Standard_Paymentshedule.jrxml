	C_BPARTNER_LOCATION.PHONE,
	C_BPARTNER_LOCATION.FAX,
	C_BPARTNER.URL,
	C_BPARTNER_LOCATION.uidnumber,
	zssi_getprojectorcostcentervalue(c_invoice.c_project_id,'') as costunit,
	zssi_getprojectorcostcentervalue('',c_invoice.a_asset_id) as costcenter,
	C_Location_Description(AD_ORGINFO.C_LOCATION_ID) AS LOCATION,
	C_CURRENCY.ISO_CODE AS CURRENCY_CODE,C_CURRENCY.CURSYMBOL AS SYMBOL,
        	C_INVOICE.DESCRIPTION,
	zssi_strDate(C_INVOICE.DATEINVOICED,$P{LANGUAGE}) as DATEORDERED,
	C_INVOICE.DOCUMENTNO,
	'' as datepromised,
	zssi_tinymce2jsreportsHTML(C_PAYMENTTERM.DESCRIPTION) AS PAYTERM,
        	zssi_strDate(current_date,$P{LANGUAGE}) as curdate,
	null as delcity,
	null as delcountry,
	null as deladdr1,
	null as deladdr2,
	null as delpostal,
	zssi_2html(zssi_getpaymentterm(C_INVOICE.C_PAYMENTTERM_ID,$P{LANGUAGE})) as paymentterm,
	zssi_getlocationcountry(C_INVOICE.C_BPARTNER_LOCATION_ID,$P{LANGUAGE}) as bpcountry,
        	zssi_2html(zssi_getexportterm(C_INVOICE.C_BPARTNER_LOCATION_ID, c_invoice.c_doctypetarget_id,$P{LANGUAGE})) as exportterm,
	c_invoice.poreference as eori,
	null as incoterms,
	null as shipper,
	null as deliverymethod,
        	CASE when $P{LANGUAGE}='de_DE' then 'Zahlungsmethode: ' || zssi_getpaymethod(C_INVOICE.paymentrule,C_INVOICE.deliveryrule,$P{LANGUAGE})
        	ELSE ' Method of payment: ' || zssi_getpaymethod(C_INVOICE.paymentrule,C_INVOICE.deliveryrule,$P{LANGUAGE})
        	END as paymethod,
	null as weight,
	null as weight_uom,
        	null as trackingno,
	null as shippingto,
        	ad_image.binarydata as logo,
	c_invoice.issotrx as issotrx,
        	CASE when c_invoice.ispaid='N' and c_invoice.deliveryrule='R' then 'Proforma-' || zssi_GetDocText(C_INVOICE.c_doctype_id,$P{LANGUAGE})
        	ELSE zssi_GetDocText(C_INVOICE.c_doctype_id,$P{LANGUAGE})
        	END  as title,
        	c_invoice.c_doctype_id as doctype_id,
        	zspr.addressheader,
	zspr.address1,
	zspr.address2,
	zspr.address3,
	zspr.address4,
	zspr.address5,
	zspr.address6,
	zspr.address7,
	zspr.address8,
	zspr.address9,
	zspr.footer1,
	zspr.footer2,
	zspr.footer3,
	zspr.footer4,
	zspr.footer5,
	zspr.onelineadr,
	zspr.isfootergroupstyle,
        	zssi_tinymce2jsreportsHTML(zssi_getTextModuleFromDoc(C_INVOICE.C_INVOICE_ID,'INVOICE','N', $P{LANGUAGE})) as textmodule1,
        	zssi_tinymce2jsreportsHTML(zssi_getTextModuleFromDoc(C_INVOICE.C_INVOICE_ID,'INVOICE','Y', $P{LANGUAGE})) as textmodule2,
	to_char(c_invoice.performanceperiodstart,'DD.MM.YYYY') as performanceperiodstart,
	CASE when (c_invoice.performanceperiodend is null) then ''
	ELSE ' - '||to_char(c_invoice.performanceperiodend,'DD.MM.YYYY')
	END as performanceperiodend,
	zspr.performacedateoninvoice as performacedateoninvoice
FROM 	C_INVOICE 		LEFT JOIN C_BPARTNER ON C_INVOICE.C_BPARTNER_ID = C_BPARTNER.C_BPARTNER_ID
                 		LEFT JOIN C_BPARTNER_LOCATION ON C_INVOICE.C_BPARTNER_LOCATION_ID = C_BPARTNER_LOCATION.C_BPARTNER_LOCATION_ID
                 		LEFT JOIN C_LOCATION ON C_BPARTNER_LOCATION.C_LOCATION_ID = C_LOCATION.C_LOCATION_ID
                 		LEFT JOIN C_PAYMENTTERM ON C_INVOICE.C_PAYMENTTERM_ID=C_PAYMENTTERM.C_PAYMENTTERM_ID
                 		LEFT JOIN C_CURRENCY ON C_INVOICE.C_CURRENCY_ID = C_CURRENCY.C_CURRENCY_ID,
        	AD_ORGINFO 	LEFT JOIN ad_image on AD_ORGINFO.your_company_document_image=ad_image.ad_image_id,
        	zspr_printinfo zspr
WHERE 	C_INVOICE.ad_org_id = AD_ORGINFO.ad_org_id AND
     	zspr.ad_org_id=C_INVOICE.ad_org_id AND
     	C_INVOICE.C_INVOICE_ID IN ($P{DOCUMENT_ID})
