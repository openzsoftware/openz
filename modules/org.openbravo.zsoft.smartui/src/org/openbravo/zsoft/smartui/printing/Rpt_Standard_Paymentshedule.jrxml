	<queryString>
		<![CDATA[SELECT 	'1' as abschnitt,
	C_ORDER.C_ORDER_ID as ID,       c_bpartner_location.printcontactname,
	zssi_tinymce2jsreportsHTML(C_ORDER.DESCRIPTION) as DESCRIPTION, C_ORDER.DOCUMENTNO as DOCUMENTNO,
	CASE when c_bpartner_location.deviant_bp_name='' or c_bpartner_location.deviant_bp_name is NULL then C_BPARTNER.NAME else c_bpartner_location.deviant_bp_name end as PARTNER,
	C_BPARTNER.VALUE as CUSTOMERNO,
	zssi_getusernamecomplete(c_order.ad_user_id,$P{LANGUAGE}) as CONTACT_NAME,
	C_LOCATION.ADDRESS1 AS ADDRESS_CS,
	C_LOCATION.ADDRESS2 AS ADDRESS_2,
	C_LOCATION.POSTAL,
	C_LOCATION.CITY,
	C_BPARTNER.owncodeatpartnersite,
	zssi_getusernamecomplete(c_order.salesrep_id,$P{LANGUAGE}) as ourrepresentative,
	zssi_getuseremail(c_order.salesrep_id) as ourrepemail,
	zssi_getuserphone(c_order.salesrep_id) as ourrepphone,
        	C_BPARTNER_LOCATION.PHONE,C_BPARTNER_LOCATION.FAX,C_BPARTNER.URL,
	C_BPARTNER_LOCATION.uidnumber,
	zssi_getprojectorcostcentervalue(c_order.c_project_id,'') as costunit,
	zssi_getprojectorcostcentervalue('',c_order.a_asset_id) as costcenter,
        	C_Location_Description(AD_ORGINFO.C_LOCATION_ID) AS LOCATION,
	C_CURRENCY.ISO_CODE AS CURRENCY_CODE,
	C_CURRENCY.CURSYMBOL AS SYMBOL,
        	C_ORDER.DESCRIPTION, zssi_strDate(C_ORDER.DATEORDERED,$P{LANGUAGE}) as DATEORDERED,
	C_ORDER.DOCUMENTNO, zssi_strDate(c_order.datepromised,$P{LANGUAGE}) as datepromised,
	zssi_2html('') as PAYTERM,
        	zssi_strDate(current_date,$P{LANGUAGE}) as curdate,
	CASE when c_order.issotrx='Y' then zssi_getlocationcity(C_ORDER.DELIVERY_LOCATION_ID) else zssi_getcityfromwarehouse(c_order.m_warehouse_id,'Y') end as delcity,
	CASE when c_order.issotrx='Y' then zssi_getlocationcountry(C_ORDER.DELIVERY_LOCATION_ID,$P{LANGUAGE}) else zssi_getcountryfromwarehouse(c_order.m_warehouse_id,$P{LANGUAGE},'Y') end  as delcountry,
	CASE when c_order.issotrx='Y' then zssi_getlocationline1(C_ORDER.DELIVERY_LOCATION_ID) else zssi_getlocline1fromwarehouse(c_order.m_warehouse_id,'Y') end  as deladdr1,
	CASE when c_order.issotrx='Y' then zssi_getlocationline2(C_ORDER.DELIVERY_LOCATION_ID) else zssi_getlocline2fromwarehouse(c_order.m_warehouse_id,'Y') end  as deladdr2,
	CASE when c_order.issotrx='Y' then zssi_getlocationpostal(C_ORDER.DELIVERY_LOCATION_ID) else zssi_getpostalfromwarehouse(c_order.m_warehouse_id,'Y') end  as delpostal,
	zssi_getpaymentterm(C_ORDER.C_PAYMENTTERM_ID,$P{LANGUAGE}) as paymentterm,
	zssi_getlocationcountry(C_ORDER.C_BPARTNER_LOCATION_ID,$P{LANGUAGE}) as bpcountry,
        	zssi_getexportterm(C_ORDER.C_BPARTNER_LOCATION_ID, c_order.c_doctypetarget_id,$P{LANGUAGE}) as exportterm,
	zssi_getlocationeori(C_ORDER.C_BPARTNER_LOCATION_ID) as eori,
	zssi_getincoterms(C_ORDER.c_incoterms_id) as incoterms,
	zssi_getshipper(C_ORDER.m_shipper_id) as shipper,
	zssi_getdeliverymethod(C_ORDER.deliveryviarule,$P{LANGUAGE}) as deliverymethod,
        	CASE when $P{LANGUAGE}='de_DE' then 'Zahlungsmethode: ' || zssi_getpaymethod(C_ORDER.paymentrule,C_ORDER.deliveryrule,$P{LANGUAGE})
        	ELSE ' Method of payment: ' || zssi_getpaymethod(C_ORDER.paymentrule,C_ORDER.deliveryrule,$P{LANGUAGE})
        	END as paymethod,
	zssi_strint(c_order.weight,$P{LANGUAGE}) as weight,
	zssi_getuom(c_order.weight_uom,$P{LANGUAGE}) as weight_uom,
        	null as trackingno,
	null as shippingto,
        	ad_image.binarydata as logo,
	c_order.issotrx as issotrx,
        	zssi_GetDocText(C_ORDER.c_doctype_id,$P{LANGUAGE}) as title,
        	c_order.c_doctype_id as doctype_id,
        	zspr.addressheader,
	zspr.address1,
	zspr.address2,
	zspr.address3,
	zspr.address4,
	zspr.address5,
	zspr.address6,
	zspr.address7,
	zspr.address8,
	zspr.address9,
	zspr.footer1,
	zspr.footer2,
	zspr.footer3,
	zspr.footer4,
	zspr.footer5,
	zspr.onelineadr,
	zspr.isfootergroupstyle,
        	zssi_tinymce2jsreportsHTML(zssi_getTextModuleFromDoc(C_ORDER.C_ORDER_ID,'ORDER','N', $P{LANGUAGE})) as textmodule1,
        	zssi_tinymce2jsreportsHTML(zssi_getTextModuleFromDoc(C_ORDER.C_ORDER_ID,'ORDER','Y', $P{LANGUAGE})) as textmodule2,
	null as performanceperiodstart,
	null as performanceperiodend,
	zspr.performacedateoninvoice as performacedateoninvoice
FROM 	C_ORDER 		LEFT JOIN C_BPARTNER ON C_ORDER.C_BPARTNER_ID = C_BPARTNER.C_BPARTNER_ID
          			LEFT JOIN C_BPARTNER_LOCATION ON C_ORDER.C_BPARTNER_LOCATION_ID = C_BPARTNER_LOCATION.C_BPARTNER_LOCATION_ID
                 		LEFT JOIN C_LOCATION ON C_BPARTNER_LOCATION.C_LOCATION_ID = C_LOCATION.C_LOCATION_ID
                 		LEFT JOIN C_PAYMENTTERM ON C_ORDER.C_PAYMENTTERM_ID=C_PAYMENTTERM.C_PAYMENTTERM_ID
                 		LEFT JOIN C_CURRENCY ON C_ORDER.C_CURRENCY_ID = C_CURRENCY.C_CURRENCY_ID,
        	AD_ORGINFO 	LEFT JOIN ad_image on AD_ORGINFO.your_company_document_image=ad_image.ad_image_id,
        	zspr_printinfo zspr
WHERE 	C_ORDER.ad_org_id = AD_ORGINFO.ad_org_id AND
    	zspr.ad_org_id=C_ORDER.ad_org_id AND
    	C_ORDER.C_ORDER_ID IN ($P{DOCUMENT_ID})
UNION
SELECT 	'2' as abschnitt,
	C_INVOICE.C_INVOICE_ID as ID,        c_bpartner_location.printcontactname,
	zssi_tinymce2jsreportsHTML(C_INVOICE.DESCRIPTION) as DESCRIPTION,
	C_INVOICE.DOCUMENTNO as DOCUMENTNO,
	CASE when c_bpartner_location.deviant_bp_name='' or c_bpartner_location.deviant_bp_name is NULL then C_BPARTNER.NAME else c_bpartner_location.deviant_bp_name end as PARTNER,
	C_BPARTNER.VALUE as CUSTOMERNO,
	zssi_getusernamecomplete(c_invoice.ad_user_id,$P{LANGUAGE}) as CONTACT_NAME,
	C_LOCATION.ADDRESS1 AS ADDRESS_CS, C_LOCATION.ADDRESS2 AS ADDRESS_2,
       	C_LOCATION.POSTAL, C_LOCATION.CITY,
	C_BPARTNER.owncodeatpartnersite,
	zssi_getusernamecomplete(c_invoice.salesrep_id,$P{LANGUAGE}) as ourrepresentative,
	zssi_getuseremail(c_invoice.salesrep_id) as ourrepemail,
